{
  "name": "common-query",
  "version": "1.10.6",
  "description": "Base framework for common mongo-style query language",
  "main": "./dist/lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zipscene/common-query.git"
  },
  "keywords": [
    "query",
    "common-query"
  ],
  "scripts": {
    "lint": "node ./node_modules/.bin/eslint . || exit 0",
    "docs": "node ./node_modules/.bin/yuidoc .",
    "test": "npm run-script build && node ./node_modules/.bin/mocha ./dist/test -R spec --recursive",
    "build": "rm -rf dist && ./node_modules/.bin/broccoli build dist && rm -rf tmp",
    "prepublish": "npm run-script build && npm run-script docs"
  },
  "dependencies": {
    "babel-runtime": "5.6.15",
    "crisphooks": "^2.0.0",
    "geolib": "^2.0.14",
    "lodash": "^3.6.0",
    "moment": "^2.10.6",
    "regexp-quote": "0.0.0",
    "xerror": "^1.0.0",
    "common-schema": "^1.8.0",
    "objtools": "^1.6.2"
  },
  "devDependencies": {
    "broccoli": "^0.16.3",
    "broccoli-babel-transpiler": "^5.2.3",
    "broccoli-cli": "^1.0.0",
    "broccoli-merge-trees": "^0.2.1",
    "broccoli-static-compiler": "^0.2.1",
    "broccoli-wrap": "0.0.2",
    "chai": "^2.2.0",
    "es6-promise": "^2.0.1",
    "eslint": "^2.12.0",
    "eslint-config-zipscene": "^0.1.11",
    "mocha": "^2.2.1",
    "source-map-support": "^0.2.10",
    "yuidocjs": "^0.6.0"
  },
  "license": "Apache-2.0"
}